<Bindings>
	<Binding name="Toggle Targets +" header="BINDING_HEADER_MONKSEEMONKDO" category="ADDONS">
		MonkSeeMonkDo_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		MonkSeeMonkDo_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		MonkSeeMonkDo_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		MonkSeeMonkDo_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		MonkSeeMonkDo_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 4" category="ADDONS">
		MonkSeeMonkDo_SetTargetMode(4)
	</Binding>
	<Binding name="Set Targets to 5+" category="ADDONS">
		MonkSeeMonkDo_SetTargetMode(5)
	</Binding>
</Bindings>
